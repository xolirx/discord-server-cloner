{
  "app": {
    "name": "Discord Server Cloner Pro",
    "version": "3.0.0",
    "author": "qtiq0",
    "telegram": "@qtiq0",
    "discord": "zlafik",
    "description": "Professional Discord server cloning tool"
  },
  "api": {
    "base_url": "https://discord.com/api/v9",
    "timeout": 30,
    "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
  },
  "limits": {
    "max_retries": 3,
    "channel_delay": 1.2,
    "role_delay": 1.5,
    "bulk_delay": 2.0,
    "rate_limit_buffer": 0.5
  },
  "features": {
    "clone_server_name": true,
    "clone_server_icon": true,
    "clone_roles": true,
    "clone_channels": true,
    "delete_existing": true,
    "preserve_order": true
  },
  "validation": {
    "min_token_length": 50,
    "snowflake_min_value": 10000000000000000,
    "max_name_length": 100
  }
}
